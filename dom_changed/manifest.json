{
    "name": "Get pages source",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Get pages source when it changes",
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "run_at": "document_end",
          "js": ["myScript.js"]
        }
      ]
    , "permissions": ["tabs", "<all_urls>"]
  }
